<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.zhangxd.trip.infrastructure.mapper.TripUserMapper">

    <select id="findUsersLike" parameterType="String" resultType="TripUser">
        SELECT
            id       "id",
            `name` "username"
        FROM trip_user
    </select>

    <select id="findList" resultType="TripUser">
        SELECT
            id       "id",
            `name` "username"
        FROM trip_user
    </select>
</mapper>
