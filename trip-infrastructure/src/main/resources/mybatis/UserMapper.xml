<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.zhangxd.trip.infrastructure.mapper.UserMapper">

    <select id="findUsersLike" parameterType="String" resultType="User">
        SELECT
            id       "id",
            username "username"
        FROM boot_user
    </select>

    <select id="findList" resultType="User">
        SELECT
            id       "id",
            username "username"
        FROM boot_user
    </select>
</mapper>
